target 'FrontendApplication' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  platform :ios, '15.5'

  # Pods for FrontendApplication
  pod 'Swinject'

  target 'FrontendApplicationUnitTests' do
    inherit! :search_paths
    # Pods for testing
  end

end

target 'FrontendAppKit' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  platform :ios, '15.5'
  
  # Pods for FrontendAppKit
  pod 'Swinject'
  pod 'RxSwift'
  pod 'RxRelay'
  pod 'RxCocoa'

  target 'FrontendAppKitUnitTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxSwift'
    pod 'RxRelay'
    pod 'Swinject'
    pod 'RxTest'
    pod 'Nimble'    
  end

end

